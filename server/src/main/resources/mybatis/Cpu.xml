<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="cn.batchfile.stat.server.dao.CpuDao" >

	<insert id="insertCpuData" parameterType="CpuData">
		REPLACE INTO `cpu_data`
		(`agent_id`,`time`,`user`,`sys`,`nice`,`idle`,`wait`,`irq`,`soft_irq`,`stolen`,`combined`) 
		VALUES 
		(#{agentId},#{time},#{user},#{sys},#{nice},#{idle},#{wait},#{irq},#{softIrq},#{stolen},#{combined})
	</insert>
	
</mapper>